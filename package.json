{
  "name": "vite-plugin-build-timestamp",
  "version": "1.0.0",
  "description": "Viteプラグイン: ビルド時にindex.htmlへタイムスタンプを埋め込む",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run tsup",
    "dev": "bun run tsup --watch",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "build",
    "timestamp",
    "html"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vite-plugin-build-timestamp.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vite-plugin-build-timestamp/issues"
  },
  "homepage": "https://github.com/yourusername/vite-plugin-build-timestamp#readme",
  "peerDependencies": {
    "vite": ">=3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}